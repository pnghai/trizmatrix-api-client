<section class="list-principles">
  <div class="col-md-4">
    <ul>
      {{#each model as |principle|}}
          <li>
              {{#link-to "principles.show" principle}}{{principle.title}}{{/link-to}}
          </li>
      {{/each}}
    </ul>

    {{#each-in model.meta.pagination as |key value|}}
      {{link-to key "principles" (query-params page=value.number)}}
    {{/each-in}}
  </div>
  <div class="col-md-8">
      <div class="list-group">
        {{outlet}}
      </div>
  </div>
</section>